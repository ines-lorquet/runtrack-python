L=[22.4, 4.0, 16.22, 9.10, 11.00, 12.22, 14.20, 5.20, 17.50]
def compte(l):
  longueur = 0
  for nb in l:
    longueur+=1
  return longueur

def arrondi(l):
    for i in range(compte(l)):
        if l[i]+0.50<l[i]-1:
           print("inferieur")
        elif l[i]+0.50<l[i]+1:
           print("superieur")
           
arrondi(L)
