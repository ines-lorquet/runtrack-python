L=[8, 24, 27, 48, 2,16, 9, 102, 7, 84, 91]

def max_min():
    max=L[0]
    min=L[0]
    for i in range(len(L)):
        if max<L[i]:
            max=L[i]
        elif min>L[i]:
            min=L[i]
        else:
            continue
    print("valeur max de la liste",max)
    print("valeur min de la liste",min)
max_min()
